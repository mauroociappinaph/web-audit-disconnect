# PageSpeed Insights API Configuration
# Get your API key from: https://developers.google.com/speed/docs/insights/v5/get-started
PSI_API_KEY=your_pagespeed_insights_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Configuration (for SaaS features)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Database Configuration (for future use)
DATABASE_URL=sqlite:./database.db
DATABASE_TYPE=sqlite

# Rate Limiting
RATE_LIMIT_MAX=100

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Queue Configuration (Redis - for future use)
REDIS_URL=redis://localhost:6379
QUEUE_CONCURRENCY=2
QUEUE_TIMEOUT=300000

# Logging
LOG_LEVEL=info
